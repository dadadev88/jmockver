{
  "name": "jmockver",
  "version": "2.0.0-rc.1",
  "description": "Run a mock server with JSON files, customize the response body and status code, all with JSON",
  "main": "./lib/index.js",
  "bin": {
    "jmockver": "./bin/jmockver",
    "jmockver-gen": "./bin/jmockverg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dadadev88/jmockver"
  },
  "scripts": {
    "dev": "nodemon dist/server/jmockver.js",
    "prewatch": "mkdir -p dist/templates && cp -r src/lib/templates/ dist/templates",
    "watch": "tsc --watch",
    "package": "sh pack.sh"
  },
  "keywords": [
    "json",
    "mock",
    "localhost",
    "serverlocal",
    "local",
    "jsonmock",
    "mock-server",
    "json-mock-server",
    "json-mock-server-cli",
    "json-mock-server-cli-tool",
    "jmockver"
  ],
  "author": "dadadev88",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "minimist": "1.2.8",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.20",
    "@types/minimist": "^1.2.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.9",
    "@types/nodemon": "^1.19.4",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "minify-all-js": "^0.1.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
